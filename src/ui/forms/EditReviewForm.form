<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="ui.forms.EditReviewForm">
  <grid id="27dc6" binding="mainPanel" layout-manager="GridBagLayout">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties>
      <font size="14"/>
    </properties>
    <border type="none"/>
    <children>
      <component id="8951f" class="javax.swing.JTextField" binding="reviewName">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="-1"/>
          </grid>
          <gridbag weightx="1.0" weighty="0.0"/>
        </constraints>
        <properties>
          <font size="16"/>
        </properties>
      </component>
      <grid id="49f82" binding="itemsPanel" layout-manager="GridBagLayout">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="2" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <gridbag weightx="5.0" weighty="5.0"/>
        </constraints>
        <properties>
          <font size="16"/>
        </properties>
        <border type="none"/>
        <children/>
      </grid>
      <component id="81dc8" class="javax.swing.JButton" binding="OKButton" default-binding="true">
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag weightx="1.0" weighty="0.0"/>
        </constraints>
        <properties>
          <text value="OK"/>
        </properties>
      </component>
      <component id="5380a" class="javax.swing.JButton" binding="cancelButton">
        <constraints>
          <grid row="3" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag weightx="1.0" weighty="0.0"/>
        </constraints>
        <properties>
          <text value="Cancel"/>
        </properties>
      </component>
      <scrollpane id="453c3" binding="newItemScrollPane" custom-create="true">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="2" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <gridbag weightx="1.0" weighty="1.0"/>
        </constraints>
        <properties/>
        <border type="none"/>
        <children>
          <component id="f1ec3" class="javax.swing.JTextArea" binding="newReviewItemText">
            <constraints/>
            <properties>
              <font size="14"/>
              <lineWrap value="true"/>
              <rows value="1"/>
            </properties>
          </component>
        </children>
      </scrollpane>
    </children>
  </grid>
</form>
